{
  "schemaVersion": 1,
  "id": "character_sheet",
  "version": "${version}",
  "name": "Character Sheet",
  "description": "A mod modyfing most of the aspect of a player for a more roleplay experience.",
  "authors": [
    "Diamssword"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/character-sheet",
    "sources": "https://github.com/Diamssword/character_sheets_mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/character_sheet/icon.png",
  "environment": "*",
  "entrypoints": {
